<?xml version="1.0" encoding="utf-8"?>
<jdiction>
  <component>com_product</component>
  <sections>
    <section>
      <tables>
        <table>
          <name>#__product</name>
          <key>id</key>
          <file>product.php</file>
          <exportfilter>(language = '*')</exportfilter>
          <cache fullfetch="0" />
        </table>
      </tables>
      <views>
        <view name="product" list="products" layout="edit" default="true">
          <form import="product.xml">
           <fieldset name="main" label="COM_JDICTION_TRANSLATION_FIELDSET_MAIN">
              <field type="text" name="name" />                         
            </fieldset>
          </form>
        </view>
      </views>
    </section>
  </sections>
</jdiction>
